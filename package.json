{
  "name": "ElectMon",
  "version": "0.1.0",
  "description": "Free open source cross platform MongoDB console written in Node.js Electron",
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:matthewlui/ElectMon.git"
  },
  "keywords": [
    "mongodb",
    "nodejs",
    "electron"
  ],
  "scripts": {
    "start": "electron bin/main.js",
    "build-osx": "electron-packager ./ --platform=darwin --arch=x64 --version 1.2.5 --out ./bin --overwrite --icon ./src/img/ElectMon.icns",
    "build-linux": "",
    "build-all": "electron-packager ./ ElectMon --all --version 1.2.5 --out ./bin --overwrite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "electron-packager": "^7.1.0",
    "electron-prebuilt": "*",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^0.3.0"
  },
  "author": {
    "name": "Matthew Lui",
    "email": "matthewluihk@icloud.com"
  },
  "license": "MIT",
  "dependencies": {
    "path": "^0.12.7",
    "request": "^2.72.0",
    "mongoose": "*"
  },
  "homepage": "https://github.com/matthewlui/ElectMon"
}
